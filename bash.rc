#
# ~/.bashrc
#

# If not running interactively, don't do anything
[[ $- != *i* ]] && return

#export PS1="\h# \e[34;1m\w\e[0m\n\$ "

if [[ ${EUID} == 0 ]] ; then
	PS1='\[\033[01m\][ \[\033[01;31m\]\u ¦ \h \[\033[00m\]\[\033[01m\]] \[\033[1;97m\]\w\[\033[00m\]\n\[\033[01;33m\][\[\033[01;31m\]$?\[\033[01;33m\][\[\033[01;31m\]$\[\033[00m\]> '
else
	PS1='\[\033[01m\][ \[\033[01;34m\]\u ¦ \h \[\033[00m\]\[\033[01m\]] \[\033[1;97m\]\w\[\033[00m\]\n\[\033[01;33m\][\[\033[01;31m\]$?\[\033[01;33m\][\[\033[01;34m\]$\[\033[00m\]> '
fi

#History
export HISTFILESIZE=20000
export HISTSIZE=10000
shopt -s histappend
shopt -s cmdhist
export HISTCONTROL=ignoredups
export HISTIGNORE="&:ls:[bf]g:exit"
export TERM="linux"


#Alias definitions
if [ -e /net/hciserver03/storage/mfrank/etc/alias ]; then
	source /net/hciserver03/storage/mfrank/etc/alias
fi

if [ -e /net/hciserver03/storage/mfrank/etc/alias_private ]; then
	source /net/hciserver03/storage/mfrank/etc/alias_private
fi

if [ -e /usr/share/doc/pkgfile/command-not-found.bash ]; then
	source /usr/share/doc/pkgfile/command-not-found.bash
fi


export PATH=/net/hciserver03/storage/mfrank/lib/crfasrnn/caffe/build/tools:$PATH
export PATH=/net/hciserver03/storage/mfrank/lib/anaconda2/bin:$PATH

export PYTHONPATH=/net/hciserver03/storage/mfrank/lib/caffe/python:$PYTHONPATH
export PYTHONPATH=/net/hciserver03/storage/mfrank/src/ifp/olympic_layer/:$PYTHONPATH
#
export LD_LIBRARY_PATH=/usr/lib/x86_64-linux-gnu:/usr/local/lib
export LD_LIBRARY_PATH=$LD_LIBRARY_PATH:/net/hciserver03/storage/mfrank/var/lib/anaconda2/lib
export LD_LIBRARY_PATH=$LD_LIBRARY_PATH:/usr/local/cuda-7.5/lib64

source /net/hciserver03/storage/mfrank/lib/venv/bin/activate
export PYTHONSTARTUP=/net/hciserver03/storage/mfrank/lib/venv/pythonstartup

