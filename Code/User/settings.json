{
    "explorer.confirmDelete": false,
    "files.associations": {
        "*.rc": "shellscript",
        "*.ml": "ocaml",
        "*.mli": "ocaml",
        "*.cfg": "yaml",
        "*.html": "html"
    },
    "gitlens.advanced.messages": {
        "suppressShowKeyBindingsNotice": true
    },
    "gitlens.historyExplorer.enabled": true,
    "editor.fontFamily": "Operator Mono Book",
    "files.exclude": {
        "**/.git": true,
        "**/.svn": true,
        "**/.hg": true,
        "**/CVS": true,
        "**/.DS_Store": true,
        "**/*~": true,
        "**/.sass-cache": true,
        "**/node_modules": false,
        "**/_build": true,
        "**/*.cmi": true,
        "**/*.aux": true,
        "**/*.fdb_latexmk": true,
        "**/*.fls": true,
        "**/*.synctex.gz": true,
        "**/__pycache__": true,
        "**/.venv": true,
        "**/.ipynb_checkpoints": true,
        "**/META-INF/": true,
        "**/CMakeFiles/": true
    },
    "editor.minimap.enabled": false,
    "editor.dragAndDrop": false,
    "editor.roundedSelection": false,
    "workbench.editor.enablePreview": false,
    "workbench.editor.enablePreviewFromQuickOpen": false,
    "window.menuBarVisibility": "toggle",
    "files.insertFinalNewline": true,
    "files.trimFinalNewlines": true,
    "files.trimTrailingWhitespace": true,
    "terminal.integrated.fontSize": 11,
    "telemetry.enableCrashReporter": false,
    "telemetry.enableTelemetry": false,
    "git.autofetch": true,
    "git.confirmSync": false,
    "workbench.startupEditor": "newUntitledFile",
    "window.zoomLevel": -2,
    "markdownlint.config": {
        "MD013": false,
        "MD033": false,
        "MD002": false
    },
    "workbench.iconTheme": "Monokai Pro (Filter Machine) Icons",
    "update.channel": "none",
    "workbench.statusBar.feedback.visible": false,
    "gitlens.gitExplorer.files.layout": "tree",
    "java.errors.incompleteClasspath.severity": "ignore",
    "latex-workshop.view.pdf.viewer": "tab",
    "latex-workshop.view.pdf.invert": 0,
    "latex-workshop.view.pdf.external.command": {
        "command": "zathura",
        "args": [
            "%PDF%"
        ]
    },
    "vsicons.dontShowNewVersionMessage": true,
    "workbench.colorTheme": "Gruvbox Dark (Medium)",
    "gitProjectManager.baseProjectsFolders": [
        "~/src/"
    ],
    "latex-workshop.latex.tools": [
        {
            "name": "latexmk",
            "command": "latexmk",
            "args": [
                "-synctex=1",
                "-interaction=nonstopmode",
                "-file-line-error",
                "-pdf",
                "-xelatex",
                "%DOC%"
            ]
        },
        {
            "name": "pdflatex",
            "command": "pdflatex",
            "args": [
                "-synctex=1",
                "-interaction=nonstopmode",
                "-file-line-error",
                "%DOC%"
            ]
        },
        {
            "name": "bibtex",
            "command": "bibtex",
            "args": [
                "%DOCFILE%"
            ]
        }
    ],
    "latex-workshop.message.update.show": false,
    "extensions.showRecommendationsOnlyOnDemand": true,
    "update.enableWindowsBackgroundUpdates": false,
    "python.jediEnabled": false,
    "python.linting.enabled": false,
    "snippet.openInNewEditor": false,
    "editor.cursorWidth": 2,
    "editor.cursorStyle": "block",
    "editor.cursorBlinking": "phase",
    "gitlens.views.repositories.files.layout": "tree",
    "gitlens.views.fileHistory.enabled": true,
    "gitlens.views.lineHistory.enabled": true,
    "breadcrumbs.enabled": true,
    "latex-workshop.chktex.enabled": true,
    "workbench.activityBar.visible": true,
    "explorer.confirmDragAndDrop": false,
    "editor.fontLigatures": true,
    "editor.fontSize": 13,
    "editor.tokenColorCustomizations": {
        "textMateRules": [
            {
                "scope": [
                    "meta.import.ts meta.block.ts variable.other.readwrite.alias.ts",
                    "meta.import.tsx meta.block.tsx variable.other.readwrite.alias.tsx",
                    "meta.import.js variable.other",
                    "meta.export.ts meta.block.ts variable.other.readwrite.alias.ts",
                    "meta.export.tsx meta.block.tsx variable.other.readwrite.alias.tsx",
                    "meta.export.js variable.other",
                    "entity.name.function.ts",
                    "entity.name.function.tsx",
                    "support.type.primitive",
                    "entity.other.attribute-name",
                    "entity.name.tag.custom",
                    "source.js.jsx keyword.control.flow.js",
                    "support.type.property.css",
                    "support.function.basic_functions",
                    "variable.assignment.coffee",
                    "support.function.basic_functions",
                    "keyword.operator.type.annotation",
                    "punctuation.section.embedded",
                    "assignment.coffee",
                    "entity.name.type.ts",
                    "support.constant.math",
                    "meta.object-literal.key",
                    "meta.var.expr storage.type",
                    "text.html.markdown",
                    "parameter",
                    "string",
                    "italic",
                    "quote",
                    "keyword",
                    "storage",
                    "language",
                    "type .function",
                    "type.function",
                    "storage.type.class",
                    "type.var",
                    "meta.parameter",
                    "variable.parameter",
                    "meta.parameters",
                    "keyword.control",
                    "modifier",
                    "this",
                    "comment"
                  ],
                  "settings": {
                    "fontStyle": "italic"
                }
            },
            {
                "scope": [
                    "meta.property-list.css meta.property-value.css variable.other.less",
                    "meta.property-list.scss variable.scss",
                    "meta.property-list.sass variable.sass",
                    "keyword.operator.logical",
                    "keyword.operator.arithmetic",
                    "keyword.operator.bitwise",
                    "keyword.operator.increment",
                    "keyword.operator.ternary",
                    "keyword.operator.comparison",
                    "keyword.operator.assignment",
                    "keyword.operator.operator",
                    "keyword.operator.or.regexp",
                    "punctuation.section.embedded.js",
                    "punctuation.definintion.string",
                    "punctuation"
                  ],
                  "settings": {
                    "fontStyle": ""
            }
            }
        ],
    }
}
